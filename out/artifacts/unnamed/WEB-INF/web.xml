    <servlet-class>uu</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>RegServlet</servlet-name>
    <servlet-class>RegServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>allUserServlet</servlet-name>
    <servlet-class>allUserServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>deleteServlet</servlet-name>
    <servlet-class>deleteServlet</servlet-class>
  </servlet>
  <servlet>
